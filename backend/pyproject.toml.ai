[project]
name = "npte_react_agent_demo"
version = "0.1.0"
description = "NPTE‑style ReAct MCQA demo (gpt‑4o‑mini, Qdrant, LangSmith)"
requires-python = ">=3.11"

dependencies = [
  "fastapi[standard]>=0.116.1",
  "uvicorn>=0.35.0",
  "langchain-core>=0.3.27",
  "langgraph>=0.0.37",
  "langsmith>=0.4.10",
  "langchain-anthropic>=0.3.18",
  "langchain-openai>=0.1.20",
  "openai>=0.27.0",
  "qdrant-client>=1.15.1",
  "python-dotenv>=1.0",
  "PyPDF2>=3.0.0",
  "python-docx>=0.8.11",
  "langchain>=0.3.0",
  "cohere>=4.0.0",
  "langsmith>=0.4.10",
  "langchain-ollama>=0.3.6",
  "langchain-qdrant>=0.2.0",
  "ragas>=0.3.0",
  "langchain-community>=0.3.27",
  "langchain-text-splitters>=0.3.9",
  "ollama>=0.5.1",
  "requests>=2.31.0",
  "nltk>=3.8.1",
  "pandas>=2.3.1",
  "datasets>=4.0.0",
  "pymupdf>=1.23.0",
  "unstructured>=0.16.12",
  "pillow>=10.0.0",
]

[tool.uv]
# necessary to treat this as a "package‑project" so uv installs your main as a dependency
package = false
